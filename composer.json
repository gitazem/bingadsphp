{
    "name": "gitazem/bingadsphp",
    "description": "Bing Ads API Version 9 and Version 10 Client Library for PHP with examples.",
    "require": {
        "php": ">=5.6.15"
    },
    "keywords": ["bing", "bing-ads","api", "sdk"],
    "license": "Apache-2.0",
    "include-path": ["src/"],
    "authors": [
        {
            "name": "Microsoft",
            "homepage": "https://msdn.microsoft.com/en-us/library/bing-ads-php-code-examples.aspx"
        },
        {
            "name": "Gita Zem",
            "email": "gitazem@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/v10/bingads/ClientProxy.php",
            "src/v10/bingads/v10",
            "src/v9/bingads"
        ],
        "exclude-from-classmap": ["src/v9/bingads/ClientProxy.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
